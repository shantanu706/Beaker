<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beaker">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".participated"
            android:label="Participated Events"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"></activity>
        <activity android:name=".Suggestion" />
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckProgress" />
        <activity android:name=".WalletCreation" />
        <activity android:name=".Transcation_Info" />
        <activity android:name=".news" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".PayAction" />
        <activity android:name=".EventFullDesc" />
        <activity
            android:name=".HomeAct"
            android:label="Events"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity android:name=".launcher" />
        <activity android:name=".SignUp" />
        <activity
            android:name=".RegistrationForm"
            android:label="Registration Form" />
    </application>

</manifest>